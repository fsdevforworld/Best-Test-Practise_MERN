  "scripts": {
    "preinstall": "npx only-allow npm",
    "in-parallel": "concurrently --kill-others-on-fail -c 'cyan.bold,blue.bold,green.bold,yellow.bold,gray.bold'",
    "lint-prettier": "prettier --write '**/*.ts' '**/*.json'",
    "lint-tslint": "tslint --format stylish --project tsconfig.json",
    "lint-tsc": "tsc",
    "lint": "npm run in-parallel -- -n 'prettier,tslint,tsc' npm:lint-prettier npm:lint-tslint npm:lint-tsc",
    "dev": "docker-compose -p dev -f docker-compose.yml -f docker-compose.dev.yml up",
    "dev-slim": "docker-compose -p dev -f docker-compose-slim.yml -f docker-compose-slim.dev.yml up",
    "dev-d": "docker-compose -p dev -f docker-compose.yml -f docker-compose.dev.yml up -d",
    "dev-slim-d": "docker-compose -p dev -f docker-compose-slim.yml -f docker-compose-slim.dev.yml up -d",
    "dev-clean": "docker-compose -p dev -f docker-compose.yml -f docker-compose.dev.yml down",
    "dev-debug": "docker-compose -p dev -f docker-compose.yml -f docker-compose.dev.yml -f docker-compose.debug.yml up",
    "dev-rebuild": "docker-compose -p dev -f docker-compose.yml -f docker-compose.dev.yml build --build-arg NPM_TOKEN=$(cat ~/.npmrc | grep 'npm.pkg.github.com/:_auth' | cut -f2 -d'=')",
    "dev-slim-rebuild": "docker-compose -p dev -f docker-compose-slim.yml -f docker-compose-slim.dev.yml build --build-arg NPM_TOKEN=$(cat ~/.npmrc | grep 'npm.pkg.github.com/:_auth' | cut -f2 -d'=')",
    "test": "npm run test:up && npm run test:run",
    "test:run": "NODE_ENV=test ./bin/tester.sh",
    "test:teardown": "docker-compose -p test -f docker-compose.test.yml down",
    "test:unit:file": "NODE_ENV=test ts-mocha --config test/mocharc.json test/tear-down.unit.ts",
    "test:unit": "NODE_ENV=test ts-mocha --config test/mocharc.json test/tear-down.unit.ts 'test/**/*.unit.ts'",
    "test:up": "docker-compose -p test -f docker-compose.test.yml up -d",
    "purify": "tslint --format stylish --project tsconfig.json && tsc && npm run test",
    "secrets:dev": "NODE_ENV=dev local-secrets --project=dave-staging-173321 --prefix _cfgload-api --allow-missing-secrets --environment=dev",
    "secrets:fetch": "local-secrets --project=default --prefix _cfgload-api --allow-missing-secrets",
    "seed": "docker-compose -p dev exec node-api ./node_modules/.bin/ts-node ./bin/scripts/seed.js",
    "dev-script": "docker-compose -p dev exec node-api ./node_modules/.bin/ts-node",
    "dev-seed": "docker-compose -p dev exec node-api ./node_modules/.bin/ts-node ./bin/dev-seed/dev-seed up",
    "dev-seed-down": "docker-compose -p dev exec node-api ./node_modules/.bin/ts-node ./bin/dev-seed/dev-seed down",
    "migrate": "docker-compose -p dev exec node-api ./node_modules/.bin/db-migrate up -m migrations",
    "migrate-down": "docker-compose -p dev exec node-api ./node_modules/.bin/db-migrate down",
    "create-migration": "./bin/create-migration.sh",
    "staging-seed": "./bin/staging-seed.sh",
    "compile": "tsc --noEmit --diagnostics",
    "console": "./node_modules/.bin/ts-node",
    "integration-tests": "mocha --exit --timeout 30000 --config ./test/mocharc.integration.json --require ts-node/register 'test/integration-tests/**/*.ts'",
    "pubsub": "docker-compose -p dev exec node-api ./node_modules/.bin/ts-node ./bin/scripts/pubsub.ts",
    "mock-auth": "NODE_ENV=dev ./node_modules/.bin/ts-node ./bin/scripts/mock-authentication.ts"
  },
